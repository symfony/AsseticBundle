<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="assetic.filter.cjsmin.class">Assetic\Filter\CJSMinFilter</parameter>
        <parameter key="assetic.filter.cjsmin.bin">/usr/bin/jsmin</parameter>
    </parameters>

    <services>
        <service id="assetic.filter.cjsmin" class="%assetic.filter.cjsmin.class%">
            <argument>%assetic.filter.cjsmin.bin%</argument>
            <tag name="assetic.filter" alias="cjsmin" />
        </service>
    </services>
</container>

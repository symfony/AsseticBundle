<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="assetic.filter.override.class">Assetic\Filter\OverrideFilter</parameter>
        <parameter key="assetic.filter.override.source_base" />
        <parameter key="assetic.filter.override.override_base" />
    </parameters>

    <services>
        <service id="assetic.filter.override" class="%assetic.filter.override.class%">
            <tag name="assetic.filter" alias="override" />
            <argument>%assetic.filter.override.source_base%</argument>
            <argument>%assetic.filter.override.override_base%</argument>
        </service>
    </services>
</container>
